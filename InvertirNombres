    private void Sacar_Hipotenusa() {
    int CatetoA, CatetoB;
    double Hipotenusa = 0;
    
    CatetoA = Integer.parseInt(jTextField1.getText());
    CatetoB = Integer.parseInt(jTextField2.getText());
    
    CatetoA = CatetoA*CatetoA;
    CatetoB = CatetoB*CatetoB;
    
    Hipotenusa= CatetoA+CatetoB;
    Hipotenusa = Math.sqrt(Hipotenusa);
    
    jTextField3.setText(String.valueOf(Hipotenusa));
    
    }

    private void Vocales() {
    String Palabra = jTextField4.getText();
    int caracter;
    int contador = 0;
    
    
            
    for (int i = 0; i < Palabra.length(); i++)
            {
            caracter = Palabra.charAt(i);
            
            if((caracter == 65)||(caracter == 69)||(caracter == 73)||(caracter == 79)||(caracter == 85)||(caracter == 97)||(caracter == 101)||(caracter == 105)||(caracter == 111)||(caracter == 117)||(caracter == 160)||(caracter == 130)||(caracter == 161)||(caracter == 162)||(caracter == 163)||(caracter == 225)||(caracter == 233)||(caracter == 237)||(caracter == 243)||(caracter == 250))
            {contador++;} 
            else {}
            }
    if(contador ==5){ jTextField5.setText("Cumple "); }
    else {jTextField5.setText("No cumple ");}
    
    contador = 0;
    caracter = 0;
        
    }

    private void Invertir_Nombre() {
   String Nombre = jTextField6.getText();
   String Apellido = jTextField7.getText(); 
  NombreArr= new String[Nombre.length()];
  ApellidoArr= new String[Apellido.length()]; 
   int longitud=0;
   
   String Contenido="";
   
   for (int i = 0; i < Nombre.length(); i++)
            {
                NombreArr[i] = String.valueOf(Nombre.charAt(i));
            
            }
   for (int i = 0; i < Apellido.length(); i++)
            {
                ApellidoArr[i] = String.valueOf(Apellido.charAt(i));
            
            }
   Ordenar(NombreArr);
   for(int i = 0; i<Resultado.length;i++){
   Contenido = Contenido + Resultado[i];
   }
   
        Ordenar(ApellidoArr);
        Contenido = Contenido+" ";
        for(int i = 0; i<Resultado.length;i++){
   Contenido = Contenido + Resultado[i];
   }
        
        jTextField8.setText(Contenido);
   
    }
    private void Ordenar(String [] Arreglo) {
        Resultado = new String[Arreglo.length];
        String temp;
        int cant = 0;
        for(int i = Arreglo.length-1; i>=0;i--)
        {temp= Arreglo[i];
        
        Resultado[cant++] = temp;
        contenido =contenido.append(Resultado[i]);
        }
    }

    private void Regla_del_3() {
   int input1 = Integer.parseInt(jTextField9.getText());
   int input2 = Integer.parseInt(jTextField10.getText());
   int input3 = Integer.parseInt(jTextField11.getText()); 
   int input4, operacion1, operacion2, resultado ;
   
   operacion1 = input2*input3;
   operacion2 = operacion1/input1;
   
   resultado = operacion2;
   jTextField12.setText(String.valueOf(resultado));
   }
    
}
